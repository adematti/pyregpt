#ifndef _REGPT_KERNELS_
#define _REGPT_KERNELS_

typedef histo_t (kernel_bias)(FLAG a, histo_t q, histo_t kq, histo_t mu, histo_t mukkq, histo_t pk_q, histo_t pk_kq); 

typedef histo_t (kernel_A_B)(size_t m, size_t n, size_t a, histo_t k, histo_t x, histo_t kq, histo_t mu, histo_t dmu, histo_t pk_k, histo_t pk_q, histo_t pk_kq);

histo_t F2_sym(FLAG a, histo_t p2, histo_t q2, histo_t pq);

histo_t S2(histo_t mu);

histo_t D2(histo_t mu);

histo_t LFunc(histo_t k, histo_t q);

histo_t WFunc(histo_t k1, histo_t k2, histo_t k3, histo_t q);

histo_t betafunc(size_t a,histo_t z);

histo_t small_beta(histo_t k, histo_t q);

histo_t big_beta(histo_t k1,histo_t k2,histo_t k3,histo_t q); 

kernel_bias kernel_b2;

kernel_bias kernel_b22;

kernel_bias kernel_bs2;

kernel_bias kernel_b2s2;

kernel_bias kernel_bs22;

kernel_bias kernel_sigma3sq;

kernel_A_B kernel_A;

kernel_A_B kernel_B;

#endif //_REGPT_KERNELS_
